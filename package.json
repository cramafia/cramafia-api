{
    "name": "mafia-api",
    "version": "1.0.0",
    "description": "back-end for mafia project",
    "main": "dist/index.js",
    "scripts": {
        "build-ts": "tsc",
        "tsc-install": "npm i -g tsc",
        "build": "npm run build-ts && npm run lint",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "test": "npx eslint ./",
        "dev": "nodemon dist/index.js",
        "start": "node dist/index.js"
    },
    "engines": {
        "node": "12.18.3",
        "npm": "6.14.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/ordinaryask/mafia-api.git"
    },
    "keywords": [
        "mafia",
        "backend"
    ],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/ordinaryask/mafia-api/issues"
    },
    "homepage": "https://gitlab.com/ordinaryask/mafia-api#readme",
    "dependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.2",
        "bcrypt": "^5.0.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.12.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.0.4",
        "socket.io": "^4.3.1",
        "typescript": "^4.5.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^7.32.0",
        "eslint-config-google": "^0.14.0",
        "nodemon": "^2.0.12",
        "prettier": "^2.3.2"
    }
